
@{
    ViewData["Title"] = "Regulations";
    Layout = "~/Views/Shared/_Layout_TCLD.cshtml";
}
<link href="~/css/advancedsearch/advancedsearch.css" rel="stylesheet" />
<link href="~/assets/Custom css/form-input-border.css" rel="stylesheet" />

<link href="~/assets/Custom css/TCLD/Brief/ManageBrief/beo_regulations.css" rel="stylesheet" />
    <div class="card">
        <div class="card-content">
            <div class="row">
                <div class="col l12 m12 s12">
                    <h3 class="center"><b>DANH SÁCH QUY ĐỊNH</b></h3>
                    <hr>
                </div>                                
            </div>
            <div class="row">
                <div class="col l2 m12 s12">
                    <a class="btn btn-small blue darken-2 input-field" id="btn-fix"><i class="fas fa-edit"></i> Chỉnh sửa</a>
                    <a class="btn btn-small blue darken-2 modal-trigger input-field" id="btn-save" href="#modal1"><i class="fas fa-save"></i> Lưu</a>
                </div>
                <div class="col l2 m12 s12 input-field offset-l4">
                    <input type="text" placeholder="Tìm kiếm bằng mã" class='form-control autocomplete_MNV' multiple />
                </div>
                <div class="col l2 m12 s12 input-field">
                    <select class="form-control" id="gender">
                        <option value="" disabled selected>Tìm kiếm theo</option>
                        <option value="1">Cao đẳng - đại học</option>
                        <option value="2">Trung cấp</option>
                        <option value="3">Công nhân kỹ thuật</option>
                        <option value="4">Trường học</option>
                        <option value="5">Hệ học</option>
                    </select>
                </div>
                <div class="col l2 m12 s12 input-field">
                    <button class="btn blue darken-2 btn-small" id="searchButton"><i class="fas fas fa-search"></i></button>
                </div>
            </div>
            <div class="row">                
                <div class="col s12 m12 l12">
                    <div id="xonly">
                        <div class="table-responsive wrapper">
                            <table class="striped table-bordered inner" id="data">
                                <thead>
                                    <tr>
                                        <th colspan="9">Chuyên môn</th>
                                        <th rowspan="2" colspan="2">Trường học</th>
                                        <th rowspan="2" colspan="2">Hệ học</th>
                                    </tr>
                                    <tr>
                                        <th colspan="3">Cao đẳng - đại học</th>
                                        <th colspan="3">Trung cấp</th>
                                        <th colspan="3">Công nhân kỹ thuật</th>
                                    </tr>
                                    <tr>
                                        <th>Mã</th>
                                        <th>Tên chuyên môn</th>
                                        <th>Chi tiết</th>
                                        <th>Mã</th>
                                        <th>Tên chuyên môn</th>
                                        <th>Chi tiết</th>
                                        <th>Mã</th>
                                        <th>Tên chuyên môn</th>
                                        <th>Chi tiết</th>
                                        <th>Mã</th>
                                        <th>Tên chuyên môn</th>
                                        <th>Mã</th>
                                        <th>Tên chuyên môn</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<!-- Modal Structure -->
<div id="modal1" class="modal">
    <div class="modal-content">
        <h3><b>Bạn có chắc chắn muốn lưu không?</b></h3>
    </div>
    <div class="modal-footer">
        <a href="#!" class="modal-action modal-close waves-effect waves-green btn blue" id="btn_agrr">Đồng ý</a>
        <a href="#!" class="modal-action modal-close waves-effect waves-green btn red">Hủy bỏ</a>
    </div>
</div>
<!--Javascript-->
<script src="~/js/Custom JS/TCLD/Brief/Briefs_List.js"></script>
<script src="~/js/advancedsearch/advancedsearch.js"></script>
<script src="~/js/Custom JS/form-input-border.js"></script>
<!--Advance search js-->
<script src="~/js/advancedsearch/advancedsearchList.js"></script>
<script src="~/js/Custom JS/TCLD/Brief/ManageBrief/Brief_Regulations.js"></script>
<!-- Scroll booster -->
<script src="~/js/Custom JS/TCLD/Brief/ManageBrief/scrollbooster.min.js"></script>
